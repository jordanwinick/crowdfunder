<%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :goal %>
  <%= f.number_field :goal %>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  <%= f.fields_for :rewards do |rewards_form| %>
  <% end %>
  <%= f.button "Create Project" %>
<% end %>